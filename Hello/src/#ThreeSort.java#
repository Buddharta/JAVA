public class ThreeSort{
        public static void main(String[] args){
                int in1 = Integer.parseInt(args[0]);
                int in2 = Integer.parseInt(args[1]);
                int in3 = Integer.parseInt(args[2]);
                int min1 = (int)Math.min(in1,in2);
                int min2 = (int)Math.min(min1,in3);
                int max1 = (int)Math.max(in1,in2);
                int max2 = (int)Math.max(max1,in3);
                int mid = (int)Math.max(min1,Math.min(in2,in3));
                System.out.println("num1 = " + in1);
                System.out.println("num2 = " + in2);
                System.out.println("num3 = " + in3);
                System.out.println("sorted = " + min2 + "," + mid + "," + max2);
        }
}
